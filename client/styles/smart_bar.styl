.smart-bar
	position fixed
	width 180px
	right 0
	top 50px
	bottom 0
	background-color #fff
	.users
		padding-top 10px
		padding-left 10px
		ul li 
			padding-top 5px
			img 
				cursor move
			p 
				padding-left 10px
			span.status
				width 8px 
				height 8px
				border-radius 50%
				margin-right 10px
				margin-top 5px
			span.online
				background-color #27ae60
			span.offline 
				background-color #e74c3c

